{
  "name": "sn/utility",
  "description": "sn Yii2 Utilities Library",
  "type": "library",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "yiisoft/yii2": ">=2.0.11",
    "ext-zlib": ">=2.0",
    "bower/ext-lib": ">=1.0.24",
    "bower-asset/es5-shim": ">=4.5"
  },
  "authors": [
    {
      "name": "Serg Nochevny",
      "email": "serg.nochevny@engineering.sn.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "sn\\utility\\": ""
    }
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  }
}
