{
  "name": "ait/utilities",
  "description": "AIT Yii2 Utilities Library",
  "type": "library",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "yiisoft/yii2": "*"
  },
  "authors": [
    {
      "name": "Serg Nochevny",
      "email": "serg.nochevny@engineering.ait.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ait\\utilities\\": ""
    }
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  }
}
